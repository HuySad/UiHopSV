    Callback = function()
        print("Discord button clicked!")
        imageCard:SetDesc("Discord link copied to clipboard!\nInvite: discord.gg/example")
    end
})

-- 20. Not Found (Error state)
local notFound = Fetching.Main:Notfound({
    Title = "No errors detected!"
})

-- ========================================
-- TAB 3: TEST FUNCTIONS
-- ========================================
local testTab = Fetching.Tab:CreateTab({
    Title = "Test Functions", 
    Icon = "6031068421"
})

-- Section test
local testSection = Fetching.Section:CreateSection({
    Title = "Test All Elements",
    Side = "left"
})

-- Test random values
local testButton1 = Fetching.Main:CreateButton({
    Title = "Randomize All Values",
    Callback = function()
        -- Random slider
        slider1:SetValue(math.random(0, 100))
        
        -- Random toggle
        toggle1:SetValue(math.random() > 0.5)
        imageToggle1:SetValue(math.random() > 0.5)
        
        -- Random text
        local randomTexts = {
            "Hello World!",
            "Testing 123",
            "Random Text",
            "Fetching UI",
            "Amazing Library"
        }
        textbox1:SetValue(randomTexts[math.random(1, #randomTexts)])
        
        -- Random status color
        local colors = {
            Color3.fromRGB(244, 67, 54),  -- Red
            Color3.fromRGB(76, 175, 80),  -- Green  
            Color3.fromRGB(33, 150, 243), -- Blue
            Color3.fromRGB(255, 193, 7)   -- Yellow
        }
        status1:SetColor(colors[math.random(1, #colors)])
        
        print("All values randomized!")
    end
})

-- Test visibility
local testButton2 = Fetching.Main:CreateButton({
    Title = "Toggle Element Visibility",
    Callback = function()
        local visible = math.random() > 0.5
        
        label1:SetVisible(visible)
        button1:SetVisible(not visible)
        slider1:SetVisible(visible)
        dropdown1:SetVisible(not visible)
        
        print("Element visibility toggled!")
    end
})

-- Add/Remove dropdown items
local testButton3 = Fetching.Main:CreateButton({
    Title = "Add Random Weapon",
    Callback = function()
        local weapons = {"Axe", "Spear", "Crossbow", "Wand", "Katana", "Mace"}
        local weapon = weapons[math.random(1, #weapons)]
        dropdown1:AddList(weapon)
        print("Added weapon:", weapon)
    end
})

local testButton4 = Fetching.Main:CreateButton({
    Title = "Clear Weapons",
    Callback = function()
        dropdown1:Clear()
        print("All weapons cleared!")
    end
})

-- Section utilities
local utilSection = Fetching.Section:CreateSection({
    Title = "Utilities",
    Side = "right"  
})

-- Print debug info
local debugButton = Fetching.Main:CreateButton({
    Title = "Print Debug Info",
    Callback = function()
        print("=== DEBUG INFO ===")
        print("Game:", game.Name)
        print("Place ID:", game.PlaceId)
        print("Players:", #game.Players:GetPlayers())
        print("Your Username:", game.Players.LocalPlayer.Name)
        print("Current Speed:", game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character.Humanoid.WalkSpeed or "N/A")
        print("=================")
    end
})

-- Tab visibility test
local tabButton = Fetching.Main:CreateButton({
    Title = "Hide/Show Tabs",
    Callback = function()
        local hide = math.random() > 0.5
        Fetching.Tab:SetVisible("Advanced", not hide)
        print("Advanced tab:", hide and "HIDDEN" or "SHOWN")
    end
})

-- Update image test
local imageButton = Fetching.Main:CreateButton({
    Title = "Change Images",
    Callback = function()
        local images = {"4483362458", "6031068421", "6031075938"}
        local img = images[math.random(1, #images)]
        
        image1:SetImage(img)
        icon1:SetImage(img)
        imageToggle1:SetImage(img)
        
        print("Images changed to ID:", img)
    end
})

-- Section management test
local mgmtButton = Fetching.Main:CreateButton({
    Title = "Toggle Sections",
    Callback = function()
        local visible = math.random() > 0.5
        Fetching.Main:SetVisible(visible)
        print("Main section visibility:", visible)
    end
})

